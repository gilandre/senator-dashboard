{
    "name": "senator-investech/dashboard",
    "description": "Dashboard SENATOR INVESTECH",
    "type": "project",
    "require": {
        "php": "^8.0",
        "vlucas/phpdotenv": "^5.5",
        "phpmailer/phpmailer": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "authors": [
        {
            "name": "SENATOR INVESTECH",
            "email": "contact@senator-investech.com"
        }
    ],
    "minimum-stability": "stable"
}
